[1m+++ b/formCheck.js[m
[36m@@ -0,0 +1,11 @@[m
[32m+[m[32mconst send = document.getElementById("send");[m
[32m+[m[32mconst mail = document.getElementById("mail");[m
[32m+[m[32mconst nam = document.getElementById("name");[m
[32m+[m[32mconst mess = document.getElementById("message");[m
[32m+[m
[32m+[m[32mconst rgx = /^[\w-\.]+@([\w-]+\.)+[\w-]{2,4}$/g[m
[32m+[m[32msend.addEventListener("click",()=>{[m
[32m+[m[32m    nam.style.borderColor = (nam.value?"#fff":"red");[m
[32m+[m[32m    mail.style.borderColor = (mail.value.match(rgx)?"#fff":"red");[m
[32m+[m[32m    mess.style.borderColor = (mess.value?"#fff":"red");[m
[32m+[m[32m})[m
